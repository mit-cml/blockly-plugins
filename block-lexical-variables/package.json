  "directories": {
    "dist": "dist",
    "src": "src"
  },
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@blockly/dev-scripts": "^4.0.9",
    "@blockly/dev-tools": "^8.1.2",
    "blockly": "^11.2.2",
